\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid                         double updatePid( double rPos, double cPos, pidData *pd )\par
                        \{\par
                                        double error = 0.0;\par
                                        double pTerm = 0.0;\par
                                        double dTerm = 0.0;\par
                                        \par
                                        error = rPos - cPos;\par
                                        pTerm = pd->kp * error;\par
                                        pd->iTerm = constrainD( pd->iTerm + pd->ki * error, PID_MIN_ITERM, PID_MAX_ITERM );\par
                                        dTerm = pd->kd * ( error - pd->eOld );\par
                                        return constrainD( pTerm + pd->iTerm + dTerm, pd->minOut, pd->maxOut );\par
                        \}\par
}
\par
}}
